  "rules" : {
    "EqualsSelf"    : { "entries" : [ { "type" : "EqualsSelf" } ] },
    "NotEqualsSelf" : { "entries" : [ { "type" : "EqualsSelf", "inverse" : true } ] },
    "Shadows"       : { "entries" : [ { "type" : "Shadows" } ] },
    "NotShadows"    : { "entries" : [ { "type" : "Shadows", "inverse" : true } ] }
  },

  "matches" : [
    ["main", [
      { "pieces" : [["base", [ 0, 0]]] },

	  // Top Edge
      {
        "matchAllPoints" : [[[ 0, 1], "NotEqualsSelf"]],
        "subMatches" : [
          {
			"matchAllPoints" : [[[-1, 1], "NotEqualsSelf"]],
			"subMatches" : [
			  { "matchAllPoints" : [[[ 1, 1], "NotEqualsSelf"]], "pieces" : [["topEdge", [ 0, 8]]] },
			  {
				"matchAllPoints" : [[[ 1, 1], "EqualsSelf"], [[ 0, 2], "NotEqualsSelf"]],
				"pieces" : [["eastRise", [ 0, 8]]]
			  }
			]
		  },
          {
			"matchAllPoints" : [[[-1, 1], "EqualsSelf"]],
			"subMatches" : [
			  {
				"matchAllPoints" : [[[ 1, 1], "NotEqualsSelf"], [[ 0, 2], "NotEqualsSelf"]],
				"pieces" : [["westRise", [ 0, 8]]]
			  },
			  {
