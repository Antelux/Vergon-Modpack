				  { "matchAllPoints" : [[[ 0, 2], "NotEqualsSelf"]], "pieces" : [["topIndent", [ 0, 8]]] }
                ]
              }
			]
		  }
		]
	  },

	  // Left Edge
      {
        "matchAllPoints" : [[[-1, 0], "NotEqualsSelf"]],
        "subMatches" : [
	      {
		    "matchAllPoints" : [[[-1, 1], "NotEqualsSelf"], [[-1,-1], "NotEqualsSelf"]],
		    "pieces" : [["leftEdge", [-4, 0]]]
		  },
          {
			"matchAllPoints" : [[[-1, 1], "EqualsSelf"], [[-2, 0], "NotEqualsSelf"]],
			"subMatches" : [
			  { "matchAllPoints" : [[[-1,-1], "NotEqualsSelf"]], "pieces" : [["westUnder", [-8, 0]]] },
			  { "matchAllPoints" : [[[-1,-1], "EqualsSelf"]], "pieces" : [["leftIndent", [-8, 0]]] }
			]
		  }
		]
	  },

	  // Right Edge
      {
        "matchAllPoints" : [[[ 1, 0], "NotEqualsSelf"]],
        "subMatches" : [
	      {
			"matchAllPoints" : [[[ 1, 1], "NotEqualsSelf"], [[ 1,-1], "NotEqualsSelf"]],
		    "pieces" : [["rightEdge", [ 8, 0]]]
		  },
          {
			"matchAllPoints" : [[[ 1, 1], "EqualsSelf"], [[ 2, 0], "NotEqualsSelf"]],
			"subMatches" : [
			  { "matchAllPoints" : [[[ 1,-1], "NotEqualsSelf"]], "pieces" : [["eastUnder", [ 8, 0]]] },
			  { "matchAllPoints" : [[[ 1,-1], "EqualsSelf"]], "pieces" : [["rightIndent", [ 8, 0]]] }
			]
		  }
		]
	  },

	  // Bottom Edge
      {
        "matchAllPoints" : [[[ 0,-1], "NotEqualsSelf"]],
        "subMatches" : [
	      {
			"matchAllPoints" : [[[-1,-1], "NotEqualsSelf"], [[ 1,-1], "NotEqualsSelf"]],
			"pieces" : [["bottomEdge", [ 0,-4]]]
		  },
          {
			"matchAllPoints" : [[[-1,-1], "EqualsSelf"], [[ 1,-1], "EqualsSelf"], [[ 0,-2], "NotEqualsSelf"]],
			"pieces" : [["bottomIndent", [ 0,-8]]]
		  }
		]
	  },

      // SHADOWS
      { "matchAllPoints" : [[[ 0, 0], "Shadows"]], "pieces" : [["fullShadow", [0, 0]]] },
      { "matchAllPoints" : [[[ 0, 1], "Shadows"]], "pieces" : [["topShadow", [0, 0]]] },
      { "matchAllPoints" : [[[-1, 0], "Shadows"]], "pieces" : [["leftShadow", [0, 0]]] },
      { "matchAllPoints" : [[[ 0,-1], "Shadows"]], "pieces" : [["bottomShadow", [0, 0]]] },
